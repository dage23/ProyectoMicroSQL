@model ProyectoMicroSQL.Models.Diccionario
@{
    ViewBag.Title = "Configuracion";
}

<h2>Configuracion</h2>
<h1>Edicion de Diccionario</h1>

@using (Html.BeginForm("ConfiguracionDiccionarioManual", "MicroSQL", FormMethod.Post, new { enctype = "multipart/form-data" }))
{<table class="table">
        <tr>
            <th>
                SELECT
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionSelect, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Select" } })
            </th>

            <th>
                FROM 
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionFrom, new { htmlAttributes = new { @class = "form-control", placeholder = "Función From" } })
            </th>

            <th>
                DELETE
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionDelete, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Delete" } })
            </th>
        </tr>

        <tr>
            <th>
                DROP TABLE
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionDropTable, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Drop Table" } })
            </th>
            <th>
                WHERE
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionWhere, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Where" } })
            </th>

            <th>
                CREATE TABLE
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionCreateTable, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Create Table" } })
            </th>
        </tr>

        <tr>
            <th>
                INSERT INTO
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionInsertInto, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Insert Into" } })
            </th>

            <th>
                VALUES
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionValue, new { htmlAttributes = new { @class = "form-control", placeholder = "Función Value" } })
            </th>

            <th>
                GO
            </th>

            <th>
                @Html.EditorFor(model => model.FuncionGo, new { htmlAttributes = new { @class = "form-control", placeholder = "Función gO" } })
            </th>
        </tr>
    </table>
    <input type="submit" value="Guardar" class="btn btn-default">
}

<h1>Importacion de Diccionario</h1>
@using (Html.BeginForm("ImportarArchivo", "MicroSQL", FormMethod.Post, new { enctype = "multipart/form-data", placeholder = "Función Select" }))
{
    <input type="file" name="Post" />
    <input type="submit" value="Importar" class="btn btn-default" />
}
