            @Html.EditorFor(model => model.FuncionGo, new { htmlAttributes = new { @class = "form-control" } })
        </th>
    </tr>  
    </table>
    <button type="submit" value="Guardar" class="btn btn-default" </button>
}

    <h1>Importacion de Diccionario</h1>
    @using (Html.BeginForm("ConfiguracionDiccionarioManual", "MicroSQL", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input type="file" name="Post" />
        <input type="submit" value="Importar" class="btn btn-default" />
    }
